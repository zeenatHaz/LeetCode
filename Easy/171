public class Solution {
    public int TitleToNumber(string columnTitle) {
        int result = 0;
        
        Console.WriteLine('B'-'A'); //1
          Console.WriteLine('D'-'A');//3
                Console.WriteLine('C'-'A') ;;//2
        Console.WriteLine('Q'-'A'); //16
for (int i = 0; i < columnTitle.Length; i++)
 result = result * 26 + (columnTitle.ElementAt(i) - 'A' + 1);
return result;
    }
}
