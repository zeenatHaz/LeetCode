 public int[] doSum(int[] nums, int target)
        {
            List<int> res = new List<int>();
            for (int i = 0; i < nums.Length; i++)
            {
                for (int j = i + 1; j < nums.Length - 1; j++)
                {
                    if (nums[i] + nums[j] == target)
                    {
                        res.Add(i);
                        res.Add(j);
                    }
                }
            }

            return res.ToArray();
        }
